@using BuyMe.Application.Currency.Commonds.CreateEditCurrency
@model CreatEditCurrencyCommond
@{
    Layout = null;
}
<div>
    <input asp-for="CurrencyId" type="hidden" />
    <input asp-for="CompanyId" type="hidden" />
    <div class="form-row">
        <div class="form-group col-md-12">
            <div class="e-float-input e-control-wrapper">
                <input asp-for="CurrencyName" />
                <span class="e-float-line"></span>
                <label asp-for="CurrencyName" class="e-float-text e-label-top">CurrencyName</label>
            </div>
        </div>

    </div>
    <div class="form-row">
        <div class="form-group col-md-12">
            <div class="e-float-input e-control-wrapper">
                <input asp-for="CurrencyCode" />
                <span class="e-float-line"></span>
                <label asp-for="CurrencyCode" class="e-float-text e-label-top">CurrencyCode</label>
            </div>
        </div>
    </div>
    
    <div class="form-row">
        <div class="form-group col-md-12">
            <div class="e-float-input e-control-wrapper">
                <input asp-for="Description" />
                <span class="e-float-line"></span>
                <label asp-for="Description" class="e-float-text e-label-top">Description</label>
            </div>
        </div>
    </div>


</div>
<ejs-scripts></ejs-scripts>
